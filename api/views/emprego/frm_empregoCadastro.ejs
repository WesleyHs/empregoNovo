            <label for="cpf">CPF</label>
            <input type="text" class="form-control" name="cpf"  maxlength="11">
        </div>
        <div class="col-md-2 col-md-offset-1">
            <label for="rg">RG</label>
            <input type="text" class="form-control" name="rg" maxlength="11">
        </div>
        </div>
        <br>
        <!--Terceira linha-->
        <div class="row">
                <div class="col-md-4">
                        <label for="endereco">Endereço</label>
                        <input type="text" class="form-control" name="endereco">
                </div>
                <div class="col-md-1 col-md-offset-0">
                        <label for="numEnd">N°</label>
                        <input type="text" class="form-control" name="numEnd">
                </div>
        </div>
        <br>
        <div class="row">
        <div class="col-md-2 col-md-offset-0">
                <label for="bairro">bairro</label>
                <input type="text" class="form-control" name="bairro">
        </div>
        <div class="col-md-2 col-md-offset-1">
                <label for="cidade">cidade</label>
                <input type="text" class="form-control" name="cidade">
        </div>
        </div>
        <br>
        <div class="row">
            <div class="col-md-2">
                <label for="telefone">Celular</label>
                <input type="text" class="form-control" name="telefone" maxlength="11">
            </div>
            <div class="col-md-2 col-md-offset-1">
                <label for="telefone">Telefone</label>
                <input type="text" class="form-control" name="telefone" maxlength="11">
            </div>            
            </div>
        <br>
        <!--quinta linha-->
        <div class="row">
            <div class="col-md-4">
                <label for="servico">Solicitar serviço</label>
                <select type="text" class="form-control" name="servico">
                        <option>Programador</option>
                        <option>Designer gráfico</option>
                        <option>Engenheiro</option>
                        <option>Contador</option>
                        <option>Personal</option>
                </select>
            </div>
         </div>
         <br>
         <!--sexta linha-->
         <div class="row">
                <div class="col-md-5">
                    <label for="email">Email</label>
                    <input type="text" class="form-control" name="email">
                </div>
                </div>
        <!--sexta linha-->
                <div class="row">
                    <div class="col-md-5">
                  <label for="senha">Senha</label>
                  <input type="password" class="form-control" placeholder="senha">
                </div>
            </div>
            <br>
        <!--setima linha-->     
                <button type="submit" class="btn btn-default">Enviar</button> 
    </div>
    <figure>
            <img src="../imagens/cadastro.png" id="fotoCadastro" class="img-responsive" alt="Imagem responsiva">
